{
  "entrypoint": "index.html",
  "shell": "src/app-shell.html",
  "fragments": [
    "src/image-file-uploader.html",
    "src/user-search-bar.html",
    "src/view-user-form.html"
  ],
  "sources": [
    "src/**/*",
    "bower.json",
    "index.html",
    "src/app-shell.html",
    "src/image-file-uploader.html",
    "src/user-search-bar.html",
    "src/view-user-form.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "bower_components/webcomponentsjs/webcomponents-lite.js"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "js": {
        "minify": true,
        "compile": "es5",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": false,
      "preset": "es5-bundled"
    },
    {
      "name": "es6-unbundled",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": true,
        "compile": "es2015",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-unbundled"
    }
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ]
  },
  "moduleResolution": "node"
}