  "main": "index.js",
  "scripts": {
    "pub": "pub get",
    "prewatch": "yarn run pub",
    "watch": "pub run build_runner watch --output=build/",
    "prebuild": "yarn run pub",
    "build": "pub run build_runner build --output=build/",
    "preserve": "yarn run build",
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "logs": "firebase functions:log",
    "predeploy.test": "yarn run build",
    "predeploy.production": "yarn run build",
    "deploy.test": "sls deploy --stage test && firebase deploy --only functions -P test",
    "deploy.production": "sls deploy  --stage production && firebase deploy --only functions -P production"
  },
  "devDependencies": {
    "firebase-tools": "^4.0.0",
    "serverless": "^1.30.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-apigwy-binary": "^0.1.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "firebase-admin": "~5.12.1",
    "firebase-functions": "^2.0.0",
    "serverless-http": "^1.6.0"
  },
  "private": true,
  "engines": {
    "node": ">=8.0"
  }
